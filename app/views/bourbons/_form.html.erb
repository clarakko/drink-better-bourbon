<%= form_for(@bourbon) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :proof %>
  <%= f.text_field :proof %>
  <br>
  <%= f.label :distillery %>
  <%= f.text_field :distillery %>
  <br>
  <%= f.label :varietal %>
  <%= f.text_field :varietal %>

  <%= f.submit "Add Bourbon" %>
<% end %>
